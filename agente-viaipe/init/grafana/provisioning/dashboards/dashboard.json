      "targets": [
        {
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH last_up AS (\n    SELECT \n        client,\n        MAX(timestamp) AS last_up_timestamp\n    FROM \n        api_results \n    WHERE \n        up = TRUE\n    GROUP BY \n        client\n)\nSELECT \n    t.client AS \"Cliente\",\n    MIN(t.timestamp) AS \"Horário da queda\"\nFROM \n    api_results AS t\nLEFT JOIN \n    last_up AS lu ON t.client = lu.client\nWHERE \n    t.up = FALSE\n    AND (\n        lu.last_up_timestamp IS NULL OR t.timestamp > lu.last_up_timestamp\n    )\nGROUP BY \n    t.client\nORDER BY   \n    \"Horário da queda\" ASC;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
