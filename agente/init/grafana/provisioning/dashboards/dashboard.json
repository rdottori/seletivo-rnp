          "rawSql": "WITH time_series AS (\n  SELECT \n    $__timeGroup(\"timestamp\", $__interval) AS \"time\"\n  FROM \n    track_results\n  WHERE \n    \"host\" = '$Host'\n    AND $__timeFilter(\"timestamp\")\n  GROUP BY\n    \"time\"\n)\nSELECT \n  ts.\"time\",\n  COALESCE(COUNT(tr.status_code), 0) AS \"Erros (404, 500, etc)\"\nFROM \n  time_series ts\nLEFT JOIN \n  track_results tr\n  ON $__timeFilter(tr.\"timestamp\") \n  AND $__timeGroup(tr.\"timestamp\", $__interval) = ts.\"time\"\n  AND tr.status_code >= 400\nGROUP BY \n  ts.\"time\"\nORDER BY \n  ts.\"time\" ASC\n",
